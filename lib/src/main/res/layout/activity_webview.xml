<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:wheel="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ha.custcom.webview.lib.wedgit.fly.PullToRefreshView
        android:id="@+id/pull_to_refresh"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ha.custcom.webview.lib.wedgit.webview.CustomWebView
            android:id="@+id/webview"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            />
    </ha.custcom.webview.lib.wedgit.fly.PullToRefreshView>

    <ha.custcom.webview.lib.wedgit.MProgressWheel
        android:id="@+id/progress_wheel"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_gravity="center"
        android:background="@drawable/progress_round_corner"
        android:visibility="gone"
        wheel:matProg_barColor="@color/sky_background"
        wheel:matProg_progressIndeterminate="true" />

</RelativeLayout>
